{
  "name": "6fb-methodologies-workshop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "dev:basic": "next dev",
    "dev:next": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbo",
    "dev:fast": "next dev --turbo --experimental-https",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "NODE_ENV=production node server.js",
    "start:next": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:all": "rm -rf .next out node_modules/.cache && npm install",
    "members:verify": "node scripts/manage-members.js verify",
    "db:migrate": "node scripts/migrate-to-production.js",
    "db:test": "curl -s http://localhost:3000/api/test-database | jq .",
    "db:setup": "echo 'Run: psql your_database < scripts/upgrade-to-database.sql'",
    "db:seed": "ts-node database/scripts/seed-database.ts",
    "db:seed:reset": "ts-node database/scripts/seed-database.ts --reset",
    "db:seed:validate": "ts-node database/scripts/seed-database.ts --no-validate",
    "db:backup": "ts-node -e \"import db from './src/lib/database.js'; await db.createBackup(); await db.close();\"",
    "db:health": "ts-node -e \"import {getHealthCheckData} from './src/lib/database.js'; console.log(JSON.stringify(await getHealthCheckData(), null, 2)); process.exit(0);\"",
    "db:performance": "ts-node -e \"import {getPerformanceData} from './src/lib/database.js'; console.log(JSON.stringify(getPerformanceData(), null, 2)); process.exit(0);\"",
    "production:test": "curl -s -X POST http://localhost:3000/api/test-database -H 'Content-Type: application/json' -d '{\"email\": \"test@6fbmethodologies.com\", \"testType\": \"full\"}' | jq .",
    "members:sync": "node scripts/manage-members.js sync",
    "members:list": "node scripts/manage-members.js list",
    "members:stats": "node scripts/manage-members.js stats",
    "members:test": "node scripts/manage-members.js test",
    "members:help": "node scripts/manage-members.js help",
    "test:skool-webhook": "node scripts/test-skool-webhook.js test",
    "zapier:test": "node scripts/zapier-automation.js test",
    "zapier:setup": "node scripts/zapier-automation.js instructions",
    "zapier:monitor": "node scripts/zapier-automation.js monitor",
    "test:optimizations": "node scripts/test-optimizations.js",
    "test:sms": "node test-sms-notifications.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration --config __tests__/integration/jest.config.js",
    "test:integration:watch": "jest __tests__/integration --config __tests__/integration/jest.config.js --watch",
    "test:integration:coverage": "jest __tests__/integration --config __tests__/integration/jest.config.js --coverage",
    "test:integration:full": "node __tests__/run-integration-tests.ts --verbose --coverage",
    "test:integration:t013": "node __tests__/run-integration-tests.ts --suite T013",
    "test:integration:t014": "node __tests__/run-integration-tests.ts --suite T014",
    "test:integration:t015": "node __tests__/run-integration-tests.ts --suite T015",
    "test:e2e": "playwright test __tests__/e2e",
    "test:e2e:headed": "playwright test __tests__/e2e --headed",
    "test:e2e:debug": "playwright test __tests__/e2e --debug",
    "test:e2e:user-journey": "playwright test __tests__/e2e/user-journey.spec.ts",
    "test:load": "node __tests__/performance/load-testing.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "test:workbook": "npm run test:integration:full",
    "test:workbook:watch": "npm run test:integration:watch",
    "test:setup": "node __tests__/setup/test-setup.js",
    "test:contract": "jest --config __tests__/contract/jest.config.js",
    "test:contract:watch": "jest --config __tests__/contract/jest.config.js --watch",
    "test:contract:coverage": "jest --config __tests__/contract/jest.config.js --coverage",
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "quality:check": "npm run lint:check && npm run format:check && npm run type-check",
    "quality:fix": "npm run lint:fix && npm run format",
    "pre-commit": "lint-staged",
    "validate": "npm run quality:check && npm run test:unit && npm run build",
    "dev:clean": "npm run clean && npm run dev",
    "build:production": "NODE_ENV=production npm run build",
    "analyze": "npm run build:analyze",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "docker:build": "docker build -t 6fb-methodologies .",
    "docker:run": "docker run -p 3000:3000 6fb-methodologies",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.654.0",
    "@aws-sdk/s3-request-presigner": "^3.654.0",
    "@headlessui/react": "^2.1.10",
