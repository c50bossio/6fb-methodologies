    "postcss": "^8.4.39",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "stripe": "^16.1.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.6",
    "twilio": "^5.9.0",
    "typescript": "^5.5.3",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/jest": "^29.5.12",
    "eslint": "^8.57.0",
